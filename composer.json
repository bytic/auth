{
  "name": "bytic/auth",
  "description": "Auth Classes used by ByTIC Framework",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\Auth\\Legacy\\": "legacy/",
      "ByTIC\\Auth\\": "src/"
    },
    "classmap": [
      "legacy"
    ],
    "files": [
      "src/functions/application.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\Auth\\Tests\\": "tests/src",
      "ByTIC\\Auth\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "bytic/event-dispatcher": "^1.0|^2.0",
    "bytic/controllers": "^1.0|^2.0",
    "bytic/cookie": "^1.0|^2.0",
    "bytic/helpers": "^1.0|^2.0",
    "bytic/orm": "^1.0|^2.0",
    "bytic/persistent-data": "^1.0|^2.0",
    "symfony/security-core": "^5.3|^6.0|^7.0",
    "symfony/security-guard": "^5.3|^6.0|^7.0",
    "lexik/jwt-authentication-bundle": "^2.12",
    "lcobucci/jwt": "^4.1|^5.0"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
